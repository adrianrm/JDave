<assembly>
  <id>bin</id>
  <formats>
    <format>zip</format>
  </formats>
  <includeBaseDirectory>true</includeBaseDirectory>
  <dependencySets>
    <dependencySet>
      <outputDirectory>dist/</outputDirectory>
      <outputFileNameMapping></outputFileNameMapping>
      <unpack>false</unpack>
      <scope>runtime</scope>
    </dependencySet>
  </dependencySets>
  <moduleSets>
    <moduleSet>
      <includes>
        <include>org.jdave:jdave-core</include>
        <include>org.jdave:jdave-junit3</include>
        <include>org.jdave:jdave-junit4</include>
        <include>org.jdave:jdave-jemmy</include>
        <include>org.jdave:jdave-wicket</include>
        <include>org.jdave:jdave-wicket-selenium</include>
        <include>org.jdave:jdave-examples</include>
        <include>org.jdave:jdave-scala</include>
        <include>org.jdave:jdave-scala-suites</include>
        <include>org.jdave:jdave-unfinalizer</include>
        <include>org.jdave:jdave-report-plugin</include>
      </includes>
      <binaries>
        <outputDirectory>lib/</outputDirectory>
        <unpack>false</unpack>
      </binaries>
      <sources>
        <outputDirectory>src/${artifactId}</outputDirectory>
      </sources>
    </moduleSet>
  </moduleSets>
</assembly>